{
  "name": "hrms-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=development vite --mode development",
    "start-dev": "cross-env NODE_ENV=development vite --mode development",
    "start-stage": "cross-env NODE_ENV=staging vite --mode staging",
    "start-prod": "cross-env NODE_ENV=production vite --mode production",
    "host-dev": "cross-env NODE_ENV=development vite --mode development --host",
    "host-stage": "cross-env NODE_ENV=staging vite --mode staging --host --port 4200",
    "build-dev": "tsc -b && cross-env NODE_ENV=development vite build --mode development",
    "build-stage": "tsc -b && cross-env NODE_ENV=staging vite build --mode staging",
    "build-prod": "tsc -b && cross-env NODE_ENV=production vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lexical/code": "^0.24.0",
